pipeline{
    agent any    {
        stages{
            stage('Checkout'){
                echo 'Checking out project from git'
                var scm = checkout scm
            }
            stage('Build'){
                echo 'Build running'
            }
            stage('Test'){
                echo 'Tests running'
            }
            stage('Deploy'){
                echo 'Deploying web app on cloud'
                echo 'Deployment Successful'
            }
        }
    }
}