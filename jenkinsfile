pipeline{
    agent master
    {
        stages{
            stage("Checkout"){
                println("Checking out project from git")
                var scm = checkout scm
            }
            stage("Build"){
                println("Build running")
            }
            stage("Build"){
                println("Tests running")
            }
            stage("Deploy"){
                println("Deploying web app on cloud")
                println("Deployment Successful")
            }
        }
    }
}